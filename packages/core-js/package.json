{
  "name": "@okto-sdk/core-js",
  "version": "0.0.0",
  "description": "Core JS for Okto SDK",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "dev": "tsc --watch",
    "build": "tsc",
    "dev:index": "tsx src/index.ts",
    "lint": "eslint .",
    "ci": "pnpm build && pnpm check-format && pnpm check-exports && npm run lint",
    "check-format": "prettier --check .",
    "format": "prettier --write .",
    "check-types": "tsc --noEmit",
    "check-exports": "attw --pack . --ignore-rules=cjs-resolves-to-esm --profile esm-only"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "dependencies": {
    "@noble/curves": "^1.8.0",
    "@noble/hashes": "^1.7.0",
    "axios": "^1.7.9",
    "axios-retry": "^4.5.0",
    "uuid": "^11.0.4",
    "viem": "^2.22.2"
  },
  "devDependencies": {
    "@arethetypeswrong/cli": "^0.17.2",
    "@okto-sdk/eslint-config": "workspace:*",
    "@okto-sdk/prettier-config": "workspace:*",
    "@okto-sdk/typescript-config": "workspace:*",
    "@types/node": "^22.10.5",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2"
  },
  "author": "Sparsh Agarwal <sparsh.a@coindcx.com>"
}
