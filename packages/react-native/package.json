{
  "name": "@okto_web3/react-native-sdk",
  "version": "0.4.1-dev.18",
  "description": "React-Native for Okto SDK",
  "main": "./dist/commonjs/index.js",
  "types": "./dist/typescript/react-native/src/index.d.ts",
  "module": "./dist/module/index.js",
  "source": "./src/index.ts",
  "exports": {
    ".": {
      "types": "./dist/typescript/react-native/src/index.d.ts",
      "import": "./dist/module/index.js",
      "require": "./dist/commonjs/index.js"
    },
    "./explorer": {
      "types": "./dist/typescript/react-native/src/explorer/index.d.ts",
      "import": "./dist/module/explorer/index.js",
      "require": "./dist/commonjs/explorer/index.js"
    },
    "./userop": {
      "types": "./dist/typescript/react-native/src/userop/index.d.ts",
      "import": "./dist/module/userop/index.js",
      "require": "./dist/commonjs/userop/index.js"
    },
    "./abstract": {
      "types": "./dist/typescript/react-native/src/abstract/index.d.ts",
      "import": "./dist/module/abstract/index.js",
      "require": "./dist/commonjs/abstract/index.js"
    },
    "./types": {
      "types": "./dist/typescript/react-native/src/index.d.ts",
      "import": "./dist/module/index.js",
      "require": "./dist/commonjs/index.js"
    }
  },
  "scripts": {
    "dev": "tsc --watch",
    "build": "bob build",
    "clean": "rimraf dist && rimraf .turbo && rimraf tsconfig.tsbuildinfo && rimraf node_modules",
    "dev:index": "tsx src/index.ts",
    "lint": "eslint .",
    "ci": "pnpm build && pnpm check-format && pnpm check-exports && npm run lint",
    "check-format": "prettier --check .",
    "format": "prettier --write .",
    "check-types": "tsc --noEmit",
    "check-exports": "attw --pack . --profile node16 --ignore-rules=cjs-resolves-to-esm"
  },
  "dependencies": {
    "@noble/ciphers": "^1.2.1",
    "@okto_web3/core-js-sdk": "workspace:*",
    "base-64": "^1.0.0",
    "react-native-mmkv": "^3.2.0"
  },
  "devDependencies": {
    "@okto_web3/eslint-config": "workspace:*",
    "@okto_web3/prettier-config": "workspace:*",
    "@okto_web3/typescript-config": "workspace:*",
    "@types/base-64": "^1.0.2",
    "@types/node": "^20.0.0",
    "@types/react": "^18.2.0",
    "@types/react-native-get-random-values": "^1.8.2",
    "babel-plugin-module-resolver": "^5.0.0",
    "react": "^18.2.0",
    "react-native-builder-bob": "^0.35.3",
    "react-native-get-random-values": "^1.11.0",
    "react-native-randombytes": "^3.6.1",
    "rimraf": "^6.0.1",
    "tsc-alias": "^1.8.10",
    "typescript": "^5.7.2"
  },
  "peerDependencies": {
    "react": "^16.0.0 || ^17.0.0 || ^18.0.0 || ^19.0.0",
    "react-native-get-random-values": "^1.11.0",
    "react-native-randombytes": "^3.6.1"
  },
  "keywords": [],
  "author": "Sparsh Agarwal <sparsh.a@coindcx.com>",
  "files": [
    "src",
    "dist",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__"
  ],
  "react-native": "src/index.ts",
  "react-native-builder-bob": {
    "source": "src",
    "output": "dist",
    "targets": [
      [
        "module",
        {
          "esm": true
        }
      ],
      [
        "commonjs",
        {
          "esm": true
        }
      ],
      "typescript"
    ],
    "plugins": [
      [
        "module-resolver",
        {
          "root": [
            "./src"
          ],
          "extensions": [
            ".ios.js",
            ".android.js",
            ".js",
            ".ts",
            ".tsx",
            ".json"
          ]
        }
      ]
    ]
  },
  "eslintIgnore": [
    "node_modules/",
    "dist/"
  ]
}
